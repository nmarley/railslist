.row
  %aside.span4
    %section
      = render 'users/user_info'
    %section
      = render 'lists/form'

  .span8
    %h3 Lists
    = form_tag root_url, method: :get, id: "lists_search" do
      = text_field_tag :search, params[:search],
        style: "width: 30%",
        autocomplete: "off",
        class: "vatop"
      = submit_tag "filter", name: nil, class: "btn vatop"

    #lists
      = render 'shared/feed'
