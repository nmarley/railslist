<%= form_tag root_url, method: :get, id: "lists_search" do %>
  <%= text_field_tag :search, params[:search],
    style: "width: 30%",
    autocomplete: "off",
    class: "vatop"
  %>
  <%= submit_tag "filter", name: nil, class: "btn vatop" %>
<% end %>
<% if @feed_items.any? %>
	<ol class="lists">
		<%= render partial: 'lists/list', collection: @feed_items %>
	</ol>
	<%= will_paginate @feed_items %>
<% end %>
