<%= provide(:title, @list.name) %>
<div class="row">
	<aside class="span4">
    <section>
      <%= render 'shared/list_info' %>
    </section>
    <section>
      <% if current_user.id == @list.user.id %>
        <%= render 'shared/item_form' %>
      <% end %>
    </section>
  </aside>
	<div class="span8">
  	<h3>Item Feed</h3>
		<%= render 'shared/itemfeed' %>
	</div>
</div>

